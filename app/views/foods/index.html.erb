<h1>You have returned <%= @results_count %> results.<h1>


<% @foods.first(10).each do |food| %>
  <div class="food">
    <p class="code">Code: <%= food.code %></p>
    <p class="description">Description: <%= food.description%></p>
    <p class="brand_owner">Brand Owner: <%= food.brand_owner%></p>
    <p class="ingredients">Ingredients: <%= food.ingredients%></p><br>
  </div>
<% end %>